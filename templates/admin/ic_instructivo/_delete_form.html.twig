<form method="post" action="{{ path('procesos_instructivo_eliminar', {'id': ic_instructivo.id}) }}"
        onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ ic_instructivo.id) }}">
    <button class="btn btn-danger btn-md">Eliminar</button>
</form>
