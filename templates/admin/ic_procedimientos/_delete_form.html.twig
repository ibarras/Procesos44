<form method="post" action="{{ path('procesos_procedimientos_eliminar', {'id': ic_procedimiento.id}) }}"
      onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ ic_procedimiento.id) }}">
    <button class="btn btn-md btn-danger">Delete</button>
</form>
